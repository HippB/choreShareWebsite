pipeline {
    agent any
    options {
        skipDefaultCheckout true
    }
    stages {
        stage('Delete workspace') {
            steps {
                deleteDir()
            }
        }
        stage('Checkout') {
            steps {
                checkout scm
            }
        }
        stage('Build') {
            steps {
                sh 'python app.py'
            }
        }
    }
}
