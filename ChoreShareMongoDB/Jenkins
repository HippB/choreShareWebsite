pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                checkout scm
            }
        }

        stage('Test') {
            steps {
                sh 'your-test-command-here'
            }
        }

        stage('Deploy') {
            steps {
                sh 'your-deploy-command-here'
            }
        }
    }
}
